{
  "name": "runtypes",
  "version": "4.0.3",
  "description": "Runtime validation for static types",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "webpack && typings",
    "format": "node scripts/format.js",
    "test": "jest $([ \"$CI\" = true ] && echo --coverage || echo --watch)",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "typings": "dts-generator --project . --out dist/runtypes.d.ts --exclude \"src/**/*.spec.ts\" --exclude \"node_modules/**/*.d.ts\""
  },
  "author": "Thomas Crockett",
  "license": "MIT",
  "devDependencies": {
    "@babel/polyfill": "^7.8.3",
    "@types/jest": "24.0.18",
    "awesome-typescript-loader": "^5.2.1",
    "coveralls": "^3.0.6",
    "dts-generator": "^3.0.0",
    "jest": "24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "3.6.3",
    "webpack-cli": "^3.3.10"
  },
  "keywords:": [
    "runtime",
    "type",
    "validation",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pelotom/runtypes"
  },
  "jest": {
    "verbose": false,
    "testRegex": ".*/*.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "dependencies": {
    "webpack": "^4.41.2"
  }
}